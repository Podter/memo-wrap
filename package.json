{
  "name": "memo-wrap",
  "version": "0.0.1",
  "description": "Cache the result of function calls",
  "license": "MIT",
  "author": {
    "name": "Nonthaphat Chaisu",
    "email": "hi@podter.me",
    "url": "https://podter.me"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./driver/*": {
      "import": "./dist/driver/*.mjs",
      "require": "./dist/driver/*.js",
      "types": "./dist/driver/*.d.ts"
    },
    "./serializer/*": {
      "import": "./dist/serializer/*.mjs",
      "require": "./dist/serializer/*.js",
      "types": "./dist/serializer/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "prettier --check . && tsc",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/node": "^22.10.7",
    "ioredis": "^5.4.2",
    "prettier": "^3.4.2",
    "superjson": "^2.2.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "unstorage": "^1.14.4"
  },
  "packageManager": "pnpm@9.15.4"
}
